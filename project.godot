; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Mancala"
run/main_scene="res://Resources/Scenes/MainMenu.scn"
config/icon="res://icon.png"

[autoload]

MainManager="*res://Scripts/Autoloads/MainManager.cs"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=960
window/size/test_height=540
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[locale]

translations=PoolStringArray( "res://Resources/Translations/Translations.en.translation", "res://Resources/Translations/Translations.pl.translation" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
